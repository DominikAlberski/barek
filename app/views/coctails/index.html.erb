<h1>Coctails</h1>

<p><%= link_to "New Coctail", new_coctail_path %></p>
<ul>
<% @coctails.each do |coctail| %>
  <li> 
    <%= link_to coctail.name, coctail_path(coctail) %> ||
    <%= link_to "delete", coctail_path(coctail), method: :delete, data: { confirm: "Cofirm delete?" }  %>
  </li>
<% end %>
</ul>